<h1>Log In</h1>

<div class="text-center">
  <%= form_for :user, url: '/users/login' do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name%><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br><br>
    <%= f.submit "Sign In" %>
  <% end %>
</div>
<br
>
<%= link_to "Login via Github", "/auth/github"%>
<br>
<%= link_to'Log in with Facebook!', '/auth/facebook' %>
<hr>
